{{layout=home}}

<div class="page-header">
	<h3><%= title %></h3>
</div>

<fieldset>
	<legend>How to Start?</legend>

	<ul class="list-group">
		<% apis.forEach(function(api){  %>
			<li class="list-group-item"> <a href="<%= this.html.url({
				controller: 'api',
				action: api.url
			}) %>"><%= api.title %></a> </li>
		<% }); %>


	</ul>
</fieldset>


